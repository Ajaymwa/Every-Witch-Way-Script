
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Every Witch Way", "DarkTheme")

local Tab = Window:NewTab("Main")
local MainSection = Tab:NewSection("Minor exploits")

MainSection:NewButton("Hide name", "ButtonInfo", function()
    print("Clicked")
    game.Players.LocalPlayer.Character.Head.Title.F.T:destroy()
end)

MainSection:NewSlider("Walk Speed", "SliderInfo", 500, 0, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)

MainSection:NewSlider("Jump Power", "SliderInfo", 500, 0, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = s
end)

local Tab = Window:NewTab("Powers")
local PowerSection = Tab:NewSection("Spam an Ability")

PowerSection:NewButton("Mass Electrocute", "ButtonInfo", function()
    print("Clicked")
    local Plr = game.Players.LocalPlayer
    for i, Player in pairs (game.Players:GetPlayers()) do

        if Player.Character ~= nil and Player ~= Plr then
            local mag = (Player.Character.UpperTorso.Position - Plr.Character.UpperTorso.Position).magnitude
            if mag < 100000 then
                local Target = Player.Character or Player.CharacterAdded:Wait()
-- Script generated by Nut The Menace Spy - credits to Claudev0#8849

local args = {
    [1] = Target
}

game:GetService("ReplicatedStorage").LightningStrikeEvent:FireServer(unpack(args))


            end
        end
end   

end)

