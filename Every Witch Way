local Tab = Window:NewTab("Keybinds")
local Section = Tab:NewSection("Kepybinds for abilities")

KeybindsSection:NewKeybind("Teleportation", "KeybindInfo", Enum.KeyCode.F, function()
	print("You just clicked the bind")
    clicktpon = true                    
    plr = game.Players.LocalPlayer
    hum = plr.Character.HumanoidRootPart
    mouse = plr:GetMouse()
    mouse.KeyDown:connect(function(key)
    if key == 'u' then
    if mouse.Target then
    if clicktpon then
    -- Script generated by Nut The Menace Spy - credits to Claudev0#8849

local args = {
    [1] = Vector3.new(mouse.Hit.x, mouse.Hit.y + 5, mouse.Hit.z)
}

game:GetService("Players").LocalPlayer.Character.Teleportation.RemoteEvent:FireServer(unpack(args))


 
    end
    end
    end
   end)
    
    --CFrame.new(Vector3.new(mouse.Hit.x, mouse.Hit.y + 5, mouse.Hit.z), Vector3.new(mouse.Hit.x, mouse.Hit.y + 5, mouse.Hit.z))
end)
